html
	head
		title!=title
		link(rel='stylesheet', href='stylesheets/main.css', type='text/css' )
	body
		h1!=message

		div(class="lala")
			for post in posts
				h2(class="url" data-url="#{post.link}" data-selector="#{selector}" data-toRemove="#{toRemove}") #{post.title}
				p #{post.link}
				p!=post.description

		div(class="rightColumn")

		script(src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.0.3/jquery.min.js")
		script(src="/javascripts/main.js")